spring:
    kafka:
        bootstrap-servers: kafka-server:9092
        producer:
            # 重试次数
            retries: 3
            # 批量发送的消息数量
            batch-size: 16384
            # 32MB的批处理缓冲区
            buffer-memory: 33554432
        consumer:
            # 默认消费者组
            group-id: group-0
            # 最早未被消费的offset
            auto-offset-reset: earliest
            # 批量一次最大拉取数据量
            max-poll-records: 100
            # 自动提交
            auto-commit-interval: 100
            enable-auto-commit: true

topicName:
    topic2: topic2Name
    topic5: topic5Name